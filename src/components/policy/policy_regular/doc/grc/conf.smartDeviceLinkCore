# source filename
regexp=\w+\.(?:cpp|h)
colours=yellow
===
regexp=^(ERROR|FATAL)
colours=red on_yellow
===
regexp=^WARN
colours=red
===
regexp=^INFO
colours=blue
===
regexp=^TRACE
colours=magenta
===
# component
regexp=(?<=\])\[[^]]+\]
colours=cyan
===
# something invalid
regexp=(?i)invalid
colours=red
===
# RPC message start
regexp=^\{$
colours=bold
count=block
===
# RPC message end
regexp=^\}$
colours=bold
count=unblock